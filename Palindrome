
# Basic Palindrome 
def palindromeOrNot(string): 
    i=0
    j=len(string)-1
    while(i<j):
        if(string[i] == string[j]):
            i+=1
            j-=1
        else:
            print("not palind", string)
            return
    print("palind", string)
            
palindromeOrNot("civic")


