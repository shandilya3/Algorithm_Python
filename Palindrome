
# Basic Palindrome 
def palindromeOrNot(string): 
    i=0
    j=len(string)-1
    while(i<j):
        if(string[i]==string[j]):
            i+=1
            j-=1
            if(i==j):
                print("palind")
                break
        else:
            print("not palind")
            break
            
palindromeOrNot("civic")

