#first unique char in the string
def UniqueChar(string):
    for char in string:
      if string.count(char)==1:
        print("This is a unique character",char)
        return true
UniqueChar("Happy")


# how it works
string2="abracadabra"
print(string2.count('a'))


** get all the substring in string **

def get_all_substrings_ofstring(input_string):
  length = len(input_string)
  return [input_string[i:j+1]
          for i in range(length)
          for j in range(i,length)]

print get_all_substrings_ofstring('happy')
